{
  "name": "argonline",
  "version": "0.26.8",
  "description": "ragnarok online clone",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsx src/index.ts",
    "prisma": "npx prisma generate",
    "migrate": "npx prisma migrate dev --name init",
    "studio": "npx prisma studio",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.ts\" --quiet --fix",
    "pre-commit": "lint-staged",
    "rseed": "npx prisma db push --force-reset &&  tsx seed/index.ts",
    "seed": "tsx seed/index.ts",
    "remigrate": "npx prisma migrate reset --force && npx prisma migrate dev --name init",
    "char": "nodemon --ext ts --exec tsx Char.ts",
    "auth": "nodemon --ext ts --exec tsx Login.ts",
    "map": "nodemon --ext ts --exec tsx Map.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NevercomeX/Argonline.git"
  },
  "author": "nevercomex",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NevercomeX/Argonline/issues"
  },
  "homepage": "https://github.com/NevercomeX/Argonline#readme",
  "dependencies": {
    "@inquirer/prompts": "^8.0.2",
    "@prisma/adapter-mariadb": "^7.1.0",
    "@prisma/adapter-pg": "^7.1.0",
    "@types/cors": "^2.8.19",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mariadb": "^3.4.5",
    "nodemon": "^3.1.11",
    "readline-sync": "^1.4.10",
    "redis": "^5.10.0",
    "socket.io": "^4.8.1",
    "typewriter-effect": "^2.22.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@prisma/client": "^7.1.0",
    "@types/cli-progress": "^3.11.6",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.0",
    "@types/ws": "^8.18.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "prettier": "3.7.4",
    "prisma": "^7.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
